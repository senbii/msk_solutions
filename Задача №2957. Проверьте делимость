
import java.util.Arrays;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt(), m = sc.nextInt();
        int[] arr = {n, m};
        Arrays.sort(arr);

        System.out.println(arr[1] % arr[0] + 1);
    }
}
